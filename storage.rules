rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null 
                  || request.origin.matches('http://localhost:.*')
                  || request.origin.matches('https://family-chore-app.vercel.app')
                  || request.origin.matches('https://family-chore-app-.*\\.vercel\\.app');
    }
  }
}
